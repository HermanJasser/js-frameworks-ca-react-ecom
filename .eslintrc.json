{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "plugins": ["react", "prettier"],
  "rules": {
    "no-console": "off",
    "no-debugger": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
